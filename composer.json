{
    "name": "justkidding96/aardvark-seo",
    "description": "Save time and get your Statamic site to rank better with the SEO addon for Statamic.",
    "version": "6.0.0",
    "type": "statamic-addon",
    "keywords": [
        "Statamic",
        "Aardvark",
        "Addon",
        "SEO",
        "Google",
        "Sitemaps",
        "Redirects",
        "Schema",
        "Tools",
        "Meta data",
        "Opengraph"
    ],
    "authors": [
        {
            "name": "justkidding96",
            "role": "Maintainer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Justkidding96\\AardvarkSeo\\": "src"
        }
    },
    "extra": {
        "statamic": {
            "name": "Aardvark SEO",
            "description": "Aardvark SEO addon"
        },
        "laravel": {
            "providers": [
                "Justkidding96\\AardvarkSeo\\ServiceProvider"
            ]
        }
    },
    "replace": {
        "withcandour/aardvark-seo": "self.version"
    },
    "require": {
        "spatie/schema-org": "^3.2",
        "statamic/cms": "^6.0",
        "whitecube/lingua": "^1.0"
    },
    "config": {
        "allow-plugins": {
            "pixelfear/composer-dist-plugin": true
        }
    }
}
